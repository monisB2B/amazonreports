# Amazon SP-API Credentials
# Variable names aligned with amazon-sp-api v2 requirements
SELLING_PARTNER_APP_CLIENT_ID=your_client_id # LWA App Client ID from Seller Central
SELLING_PARTNER_APP_CLIENT_SECRET=your_client_secret # LWA App Client Secret from Seller Central
SELLING_PARTNER_REFRESH_TOKEN=your_refresh_token # LWA Refresh Token for your seller account
AWS_SELLING_PARTNER_ACCESS_KEY_ID=your_aws_access_key # AWS IAM User Access Key
AWS_SELLING_PARTNER_SECRET_ACCESS_KEY=your_aws_secret_key # AWS IAM User Secret Key
AWS_SELLING_PARTNER_ROLE=your_role_arn # AWS IAM Role ARN for SP-API

# Region settings
SP_API_REGION=NA # NA, EU, FE
MARKETPLACE_ID=your_marketplace_id

# App settings
PORT=3000
